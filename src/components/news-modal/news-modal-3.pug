mixin news-modal-3()
    div.modal-form.news-modal(data-init="modal" data-type="modal-news-3")
        button.modal-form__close(type="button")
        .modal-form__body
            .news-modal__content
                h2.modal-form__title Майские поступления
                img.news-modal__img(src="assets/img/news-3.jpg" alt="Майские поступления" width="360" height="200")
                span.news__date 27.05.2023

