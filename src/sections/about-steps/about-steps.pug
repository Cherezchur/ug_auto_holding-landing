mixin about-steps()
    section.about-steps
        h2.about-steps__title Чтобы создать лучший экскаватор-погрузчик, мы пошли к вам
        .about-steps__wrapper
            img.about-steps__img.left.first(src="assets/img/step-2.svg" alt='Устройство кабины' width="220" height="600")
            img.about-steps__img.center(src="assets/img/step-1.svg" alt='Устройство кабины виж сверху' width="611" height="600")
            img.about-steps__img.left(src="assets/img/step-3.svg" alt='Устройство кабины' width="220" height="600")
